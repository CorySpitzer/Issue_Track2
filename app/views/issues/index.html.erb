<h1>Issues#index</h1>
<p>Find me in app/views/issues/index.html.erb</p>

<ul>
    <% @issues.each do |issue| %>
        <li>
            <h2> <%= issue.summary  %> </h2>
            <p> <%= issue.description  %> </p>
            <h3> <%= issue.status  %> </h3>
            
        </li>
    <% end %>
</ul>